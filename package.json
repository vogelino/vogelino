{
  "name": "vogelino22",
  "description": "Lucas Vogel's (Vogelino) 2023 online portfolio",
  "private": true,
  "scripts": {
    "dev": "TAILWIND_MODE=watch astro dev",
    "start": "astro dev",
    "build": "npm run code-qa && npm run downloadInspirations && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --noEmit",
    "downloadInspirations": "npx ts-node -P scripts/tsconfig.json scripts/downloadInspirations.ts",
    "lint": "eslint 'src/**/*.{js,ts,astro}'",
    "lint-fix": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "code-qa": "npm run type-check && npm run format && npm run lint-fix"
  },
  "devDependencies": {
    "@astrojs/check": "0.5.10",
    "@astrojs/mdx": "2.3.1",
    "@astrojs/rss": "4.0.5",
    "@astrojs/sitemap": "3.1.3",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/ts-plugin": "1.6.1",
    "@notionhq/client": "2.2.14",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/marked": "4.0.7",
    "@types/node-fetch": "2.6.11",
    "@types/sharp": "0.30.5",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "astro": "4.6.2",
    "autoprefixer": "10.4.14",
    "dotenv": "16.0.2",
    "eslint": "8.56.0",
    "eslint-plugin-astro": "0.27.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "node-fetch": "2.6.7",
    "postcss": "8.4.33",
    "prettier": "3.2.5",
    "prettier-config-standard": "7.0.0",
    "prettier-plugin-astro": "0.11.0",
    "sharp": "0.33.2",
    "slugify": "1.6.5",
    "tailwindcss": "3.3.6",
    "tailwindcss-touch": "1.0.1",
    "ts-node": "10.8.0",
    "typescript": "5.2.2",
    "zod": "3.22.4"
  },
  "dependencies": {
    "@playform/inline": "0.0.2",
    "@sentry/astro": "^7.110.1",
    "@splidejs/splide": "4.1.4",
    "@vercel/analytics": "1.2.2",
    "@vercel/speed-insights": "1.0.10",
    "photoswipe": "5.4.3"
  }
}